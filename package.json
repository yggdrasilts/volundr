{
  "name": "@yggdrasilts/volundr",
  "version": "0.1.8",
  "author": "David LÃ³pez <davidlopez.david@gmail.com>",
  "main": "dist/index.js",
  "contributors": [
    "David Lopez <davidlopez.david@gmail.com>"
  ],
  "license": "MIT",
  "description": "A set of utilities to do the developer live easy.",
  "keywords": [
    "typescript",
    "utilities",
    "javascript"
  ],
  "bugs": {
    "url": "https://github.com/yggdrasilts/volundr/issues"
  },
  "homepage": "https://github.com/yggdrasilts/volundr",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yggdrasilts/volundr"
  },
  "scripts": {
    "build": "npm run lint && npm test && webpack --config ./webpack.config.js",
    "build:prod": "rimraf dist && npm run lint && npm test && tsc -p tsconfig.prod.json",
    "clean:all": "rm -rf node_modules package-lock.json dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "prebuild": "rimraf dist",
    "prepublish": "npm run build:prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "typescript-char": "0.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.1.6",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "engines": {
    "node": ">= 12.3.0",
    "npm": ">= 6.9.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "files": [
    "dist",
    "LICENSE.md"
  ]
}
